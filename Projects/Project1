//Problem Progam need to output sums/products/divison of two numbers that the user entered
#include <stdio.h>
#include <stdlib.h>
#pragma warning(disable:4996)

main() {
	int a, b, sum;
	int product, division;
	printf("Enter the first number \n");
	scanf("%i", &a);
	printf("Enter the second number \n");
	scanf("%i", &b);
	sum = a + b;
	printf("Result: %i \n", sum);

	product = a * b;
	printf("Result: %i \n", product);
	
	division = a / b;
	printf("Result: %i \n", division);


	system("pause");
}
